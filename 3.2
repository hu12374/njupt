#include <stdio.h>
#include <stdlib.h>
int main()
{
	FILE* fp;
	char ch;
	fp = fopen("test.txt", "r+");
	if (fp == 0)
	{
		printf("error");
		exit(1);
		printf("end with '#'\n");
	}
	ch = getchar();
	while (ch != '#')
	{
		fputc(ch, fp);
		ch = getchar();
	}
	rewind(fp);
	while ((ch = fgetc(fp)) != EOF)
	{
		putchar(ch);
	}
	putchar('\n');
	fclose(fp);
	return 0;
}
